{
  "expo": {
    "name": "Sierra",
    "slug": "Sierra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sierra.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/sierra.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sierra.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sierra.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sierra.app",
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/sierra.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "cd6bc18b-62a8-4cc0-96e3-52fdbf3e6b4d"
      }
    }
  }
}
